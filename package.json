{
  "name": "vue-pipeline",
  "version": "1.0.12",
  "private": false,
  "author": "Zuo Jinfang <jinfang134@sina.com>",
  "license": "MIT",
  "description": "One easy-to-use component to show beautiful responsive timeline like jenkins blue ocean plugin.",
  "repository": {
    "type": "git",
    "url": "https://github.com/jinfang134/vue-pipeline"
  },
  "keywords": [
    "vue",
    "components",
    "timeline"
  ],
  "main": "index.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test": "ava test/*.test.js --verbose",
    "lint": "eslint --quiet --cache --ext .vue,.js, src/** ./demo/*.{js,vue}",
    "preversion": "npm test && npm run lint",
    "version": "git add .",
    "postversion": "git push origin master && git push --tags && npm publish --registry=https://registry.npmjs.org/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn test",
      "pre-push": "yarn lint"
    }
  },
  "ava": {
    "require": [
      "@babel/register"
    ],
    "babel": {
      "testOptions": {
        "babelrc": false,
        "configFile": false
      }
    }
  },
  "dependencies": {
    "string-width": "^5.0.0"
  },
  "devDependencies": {
    "@babel/register": "^7.13.14",
    "@vue/babel-preset-app": "^4.5.12",
    "@vue/cli-plugin-babel": "^4.5.12",
    "@vue/cli-plugin-eslint": "^4.5.12",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/cli-service": "^4.5.12",
    "ava": "^3.15.0",
    "element-plus": "^1.0.2-beta.40",
    "vue": "^3.0.11",
    "babel-eslint": "^10.1.0",
    "core-js": "^3.10.1",
    "eslint": "^7.24.0",
    "eslint-plugin-vue": "^7.9.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^6.0.0",
    "@vue/compiler-sfc": "^3.0.11"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
